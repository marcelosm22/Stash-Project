

<div class="div home-music-grid">
<div class="music-amy-home-top-div">

<% @amy = Forum.find(12) %>

<div class="home-music-img-div">
<%= link_to forum_path(@amy) do %>
<%= cl_image_tag @amy.photo.key, height: 200, width: 200 %>
<% end %>
</div>

<div class="music-home-text-div">
<p>Trending in Music</p>
<p><%= @amy.title %></p>
<p><%= @amy.creator  %></p>
</div>

<div class="home-rating-cirlce">
 <div class="c100 p100 medium small green">
          <span> 8.5 </span>
          <div class="slice">
            <div class="bar"></div>
            <div class="fill"></div>
          </div>
      </div>
</div>

</div>

